#set( $symbol_pound = '#' )
#set( $symbol_dollar = '$' )
#set( $symbol_escape = '\' )
<component name="ArtifactManager">
  <artifact type="war" name="${artifactId}-war:war">
    <output-path>${symbol_dollar}PROJECT_DIR${symbol_dollar}/${artifactId}-war/target</output-path>
    <root id="archive" name="proto-war.war">
      <element id="artifact" artifact-name="${artifactId}-war:war exploded" />
    </root>
  </artifact>
</component>